{
  "name": "react-primitives-arcade-button",
  "version": "0.1.5",
  "description": "Arcade button component for React and React Native",
  "author": "Chris Dolphin",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/likethemammal/react-primitives-arcade-button.git"
  },
  "bugs": {
    "url": "https://github.com/likethemammal/react-primitives-arcade-button/issues"
  },
  "homepage": "https://github.com/likethemammal/react-primitives-arcade-button#readme",
  "keywords": [
    "react"
